{
  "name": "seeus-dispatcher",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "startElectron": "cross-env NODE_ENV=development electron dist/main.index.js",
    "devBuildAll": "cross-env NODE_ENV=development webpack --config webpack.all.js --progress --colors",
    "watchRenderer": "cross-env NODE_ENV=development webpack --config webpack.renderer.js --watch --progress --colors",
    "dev": "npm-run-all clean devBuildAll --parallel startElectron watchRenderer",
    "prod": "yarn clean && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register '**/*.test.ts'",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "main": "main.js",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fluentui/react": "^7.105.5",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "electron": "^6.0.8",
    "electron-builder": "^22.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
